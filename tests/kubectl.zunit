#!/usr/bin/env zunit

@setup {
    load ../fzf-zsh-completions.plugin.zsh
    load _helpers/mock.zsh
    load _helpers/assertions.zsh
    mock kubectl
    mock _fzf_complete_kubectl_unknown-subcommand
    mock __fzf_extract_command
}

@teardown {
    (unmock kubectl)
    (unmock _fzf_complete_kubectl_diff)
    (unmock _fzf_complete_kubectl_unknown-subcommand)
    (unmock __fzf_extract_command)
}

@test 'Testing overridden completion: kubectl diff **' {
    mock _fzf_complete_kubectl_diff

    __fzf_extract_command_mock_1() {
        assert $# equals 1
        assert $1 same_as 'kubectl diff '

        echo 'kubectl'
    }

    _fzf_complete_kubectl_diff_mock_1() {
        assert $# equals 1
        assert $1 same_as 'kubectl diff '
    }

    _fzf_complete() {
        fail '_fzf_complete should not be invoked'
    }

    prefix=
    _fzf_complete_kubectl 'kubectl diff '

    assert _fzf_complete_kubectl_diff mock_times 1
}

