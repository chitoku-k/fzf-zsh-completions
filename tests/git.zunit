        run cat
        assert ${#lines} equals 5
        assert ${lines[1]} same_as strip
        assert ${lines[2]} same_as whitespace
        assert ${lines[3]} same_as verbatim
        assert ${lines[4]} same_as scissors
        assert ${lines[5]} same_as default
